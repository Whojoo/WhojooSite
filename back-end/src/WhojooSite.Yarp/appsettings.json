{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "web": {
        "ClusterId": "webCluster",
        "AuthorizationPolicy": "anonymous",
        "Match": {
          "Path": "{**catch-all}"
        }
      }
    },
    "Clusters": {
      "webCluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://localhost:4200"
          }
        }
      }
    }
  }
}